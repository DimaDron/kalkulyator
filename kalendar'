#include<stdio.h>
#include<stdlib.h>

int isVis(int);
int main(int argc, char *argv[])

{
    int months[] = {31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31};
    int D, M, Y, i, f, j;
    j = 1;
    if(argc < 4)
    {
     printf("argc < 4\n");
     exit(-1);
    }
     D = atoi(argv[1]);
     M = atoi(argv[2]);
     Y = atoi(argv[3]);
     for(i = 1; i < Y; i++)
    {
    if(i % 400 == 0)
       j += 2;

		else if(i % 100 == 0)
            j ++;

		else if(i % 4 == 0)
            j += 2;
        else
            j ++;

			if(j > 7) j -= 7;
    }

	j--;
    if(j == 0) j = 7;
    for(i = 0; i < M - 1; i ++)
    {
    if(i == 1 && (isVis(Y) & 1))

		{
        j++;
        continue;

		}

		else if(i == 1)
           continue;
        j += months[i] - (months[i] / 7) * 7;

		if(j > 7) j -= 7;

	}
    j += D - (D / 7) * 7;
    if(j > 7) j -= 7;
    switch(j)

	{

		case 1: printf("monday\n");
		break;

		case 2: printf("tuesday\n");
		break;

		case 3: printf("wednesday\n");
		break;

		case 4: printf("thursday\n");
		break;

		case 5: printf("friday\n");
		break;

		case 6: printf("saturday\n");
		break;

		case 7: printf("sunday\n");
		break;
    }
        return 0;

}
int isVis(int year)

{
     if(year % 400 == 0)
       return 1;

	else if(year % 100 == 0)
        return 0;

	else if(year % 4 == 0)
        return 1;

	else
        return 0;

}
